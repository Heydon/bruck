<!doctype html>
<html lang="en">
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.min.css" />
    <title>{{ pageTitle }}</title>
  </head>
  <body>
    <c-enter>
      <g-rid items="center">
        <t-riangle aspectratio="1/2"></t-riangle>
        <s-tar aspectRatio="16/9"></s-tar>
        <p-olygon sides="7" aspectratio="3/2"></p-olygon>
        <e-llipse></e-llipse>
        <r-ectangle aspectratio="16/9"></r-ectangle>
        <t-riangle></t-riangle>
        <t-riangle aspectratio="1/2"></t-riangle>
        <t-riangle></t-riangle>
      </g-rid>
    </c-enter>
    <script type="module">
      {% for module in manifest.modules %}import { {{ module.exports[0].name }} } from './js/{{ module.path }}'; {% endfor %}
      {% for module in manifest.modules %}{% set name = module.exports[0].name %}{% set tagName = module.declarations[0].tagName %}{% if tagName %}customElements.define('{{tagName}}', {{name}}); {% endif %}{% endfor %}
    </script>
  </body>
</html>